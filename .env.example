# CollectAPI Token
# Vercel'de Environment Variables'a ekleyin veya local'de .env.local dosyasına ekleyin
COLLECTAPI_TOKEN=your_collectapi_token_here

# Vercel Postgres (Production'da Vercel otomatik ekler)
# Local'de kullanmak için kendi PostgreSQL connection string'inizi ekleyin
POSTGRES_PRISMA_URL=your_postgres_prisma_url_here
POSTGRES_URL_NON_POOLING=your_postgres_direct_url_here

# Vercel KV (Production'da Vercel otomatik ekler)
# Local'de çalışmaz
# KV_REST_API_URL=
# KV_REST_API_TOKEN=
# KV_REST_API_READ_ONLY_TOKEN=
